{
    "display_information": {
        "name": "BusyLight"
    },
    "features": {
        "bot_user": {
            "display_name": "BusyLight",
            "always_online": false
        }
    },
    "oauth_config": {
        "scopes": {
            "user": [
                "users:read"
            ],
            "bot": [
                "calls:read",
                "chat:write",
                "dnd:read",
                "users:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "user_events": [
                "user_huddle_changed",
                "user_status_changed"
            ],
            "bot_events": [
                "user_huddle_changed",
                "user_status_changed"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}